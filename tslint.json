{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always"
        }
      }
    ],
    "variable-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "max-classes-per-file": false,
    "no-console": [true, "log"]
  },
  "linterOptions": {
    "exclude": ["**/*.js", "*.json"]
  }
}
